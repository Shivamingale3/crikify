{
  "expo": {
    "name": "Crikify",
    "slug": "crikify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/icon.svg",
    "scheme": "crikify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#343433",
        "foregroundImage": "./assets/images/icons/icon.svg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icons/icon.svg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/icon.svg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#343433",
          "dark": {
            "backgroundColor": "#343433"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
